{
	"name": "sveltekit-typescript",
	"version": "0.0.1",
	"scripts": {
		"build": "svelte-kit build",
		"start:dev": "svelte-kit dev --host 0.0.0.0",
		"start:preview": "svelte-kit preview --host 0.0.0.0",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"prepare": "npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme"
	},
	"devDependencies": {
		"@smui/button": "^6.0.0-beta.15",
		"@smui/card": "^6.0.0-beta.15",
		"@smui/circular-progress": "^6.0.0-beta.15",
		"@smui/layout-grid": "^6.0.0-beta.15",
		"@smui/snackbar": "^6.0.0-beta.15",
		"@smui/textfield": "^6.0.0-beta.15",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-netlify": "^1.0.0-next.51",
		"@sveltejs/adapter-static": "^1.0.0-next.28",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.4.1",
		"@typescript-eslint/eslint-plugin": "^5.18.0",
		"@typescript-eslint/parser": "^5.18.0",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.6.0",
		"smui-theme": "^6.0.0-beta.15",
		"svelte": "^3.46.6",
		"svelte-check": "^2.4.6",
		"svelte-preprocess": "^4.10.5",
		"tslib": "^2.3.1",
		"typescript": "^4.6.3"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.7",
		"@lukeed/uuid": "^2.0.0",
		"@material/theme": "^13.0.0",
		"axios": "^0.26.1",
		"cookie": "^0.4.2",
		"file-saver": "^2.0.5",
		"svelte-material-ui": "^6.0.0-beta.15"
	}
}
